<tr class="user_input">

  <th class="headcol headcol__user-row">Feedback</th>
  <% section_titles.each do |title| %>
    <td class="no_padding column_<%=title%>">
      <div id="feedback_cell_<%= title %>" class="<%= feedback_cell_class(current_user, feedbacks[title]) %>" column_value="<%= title %>" daac_curator="<%= current_user.daac_curator? %>" onclick="feedbackClickHandler(this)">
      <input style="visibility: hidden;" type="checkbox" id="feedback_check_<%= title %>" name="feedback[<%= title %>]" form="field_form" <%= feedbacks[title] ? "checked"  : "" %>>
        <p id="feedback_text_<%= title %>" class="<%= feedbacks[title] ? "review-toggle__text" : "" %>">
          <i id="feedback_icon_<%= title %>" class="<%= feedbacks[title] ? "eui-icon eui-check-o" : "fa fa-comments" %>"></i>
          Request Reviewer Feedback
        </p>
      </div>
    </td>
<% end %>

</tr>

