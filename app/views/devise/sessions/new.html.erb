<div class="page_header">
  <div class="header_title">
    <h3>WELCOME TO THE COMMON METADATA REPOSITORY (CMR) DASHBOARD <%= get_environment_display_name %></h3>
  </div>
  <div class="clear_box"></div>
</div>
 
<div class="header_image">
</div>  

<div class="log_in_box">
  <% if devise_mapping.omniauthable? %>
    <% resource_class.omniauth_providers.each do |provider| %>
      <%= link_to "Login with #{get_omniauth_provider_display_name(provider)}", omniauth_authorize_path(resource_name, provider) %><br/>
    <% end %>
  <% end %>
</div>
